    "test": "vitest run",
    "test:watch": "vitest watch",
    "generate": "tsx --tsconfig tsconfig.generate.json generate.ts",
    "prepublishOnly": "npm run build",
    "debug": "tsx --tsconfig tsconfig.debug.json debug.ts",
