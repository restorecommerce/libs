    "build:codegen:clean": "rimraf -rf ./codegen",
    "build:codegen:compile": "tsc -p tsconfig.codegen.json",
    "build:es": "node esbuild.mjs",
    "dev": "npm-run-all build:clean dev:compile dev:serve",
