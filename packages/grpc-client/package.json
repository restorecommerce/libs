{
  "name": "@restorecommerce/grpc-client",
  "version": "2.2.11",
  "description": "Generic GRPC client",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "author": "n-fuse GmbH",
  "repository": {
    "type": "git",
    "url": "https://github.com/restorecommerce/libs.git"
  },
  "license": "MIT",
  "keywords": [
    "restore",
    "commerce",
    "grpc"
  ],
  "dependencies": {
    "@grpc/grpc-js": "^1.10.10",
    "@grpc/proto-loader": "^0.7.13",
    "@restorecommerce/logger": "^1.3.6",
    "cls-rtracer": "^2.6.3",
    "nice-grpc": "^2.1.13",
    "nice-grpc-client-middleware-deadline": "^2.0.16",
    "nice-grpc-client-middleware-retry": "^3.1.12",
    "protobufjs": "^7.3.2",
    "retry": "^0.13.1",
    "rxjs": "^7.8.1",
    "winston": "^3.13.0"
  },
  "devDependencies": {
    "@restorecommerce/dev": "^0.1.1",
    "@types/lodash": "^4.17.5",
    "@types/node": "22.8.4",
    "@types/retry": "^0.12.5",
    "@types/uuid": "^10.0.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "6.0.1",
    "ts-proto": "2.2.5",
    "ts-proto-descriptors": "2.0.0",
    "typescript": "^5.5.2",
    "vitest": "^2.1.4"
  },
  "scripts": {
    "build": "npm-run-all build:clean build:compile",
    "build:clean": "rimraf -rf ./dist",
    "build:compile": "tsc -p tsconfig.lib.json",
    "test": "vitest run",
    "prepublishOnly": "npm run build",
    "generate:test": "protoc --experimental_allow_proto3_optional --ts_proto_opt=outputClientImpl=false,env=node,useOptionals=true,outputJsonMethods=true,outputSchema=true,useExactTypes=false,outputServices=nice-grpc,outputServices=generic-definitions --proto_path=./tests/protos --plugin=./node_modules/.bin/protoc-gen-ts_proto --ts_proto_out=./tests/generated ./tests/protos/echo/*.proto"
  },
  "engines": {
    "node": ">= 20.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "nx": {
    "targets": {
      "build": {
        "outputs": [
          "{projectRoot}/dist"
        ]
      }
    }
  },
  "gitHead": "0a6f7c94bfb1811c5dd60ab8c4912a41299b3717"
}
