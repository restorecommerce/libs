{
  "name": "@restorecommerce/grpc-client",
  "version": "1.0.0",
  "description": "Generic GRPC client",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "author": "n-fuse GmbH",
  "repository": {
    "type": "git",
    "url": "https://github.com/restorecommerce/libs.git"
  },
  "license": "MIT",
  "keywords": [
    "restore",
    "commerce",
    "grpc"
  ],
  "dependencies": {
    "@grpc/grpc-js": "^1.6.7",
    "@grpc/proto-loader": "^0.6.13",
    "@restorecommerce/logger": "^1.0.0",
    "cls-rtracer": "^2.6.2",
    "lodash": "^4.17.21",
    "nice-grpc": "^1.2.0",
    "nice-grpc-client-middleware-deadline": "^1.1.0",
    "protobufjs": "^6.11.3",
    "retry": "^0.13.1",
    "rxjs": "^7.5.5",
    "uuid": "^8.3.2",
    "winston": "^3.8.1"
  },
  "devDependencies": {
    "@types/jest": "^28.1.4",
    "@types/lodash": "^4.14.182",
    "@types/node": "^18.0.3",
    "@types/retry": "^0.12.2",
    "@types/uuid": "^8.3.4",
    "jest": "^28.1.2",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "ts-jest": "^28.0.5",
    "ts-proto": "^1.117.0",
    "ts-proto-descriptors": "^1.7.1",
    "typescript": "^4.7.4"
  },
  "scripts": {
    "build": "npm-run-all build:clean build:compile",
    "build:clean": "rimraf -rf ./dist",
    "build:compile": "tsc -p tsconfig.lib.json",
    "test": "jest",
    "prepublishOnly": "npm run build",
    "generate:test": "protoc --experimental_allow_proto3_optional --ts_proto_opt=outputClientImpl=false,env=node,useOptionals=true,outputJsonMethods=true,outputSchema=true,useExactTypes=false,outputServices=nice-grpc,outputServices=generic-definitions --proto_path=./tests/protos --plugin=./node_modules/.bin/protoc-gen-ts_proto --ts_proto_out=./tests/generated ./tests/protos/echo/*.proto"
  },
  "engines": {
    "node": ">= 12.18.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "nx": {
    "targets": {
      "build": {
        "outputs": [
          "./dist"
        ]
      }
    }
  },
  "gitHead": "0a6f7c94bfb1811c5dd60ab8c4912a41299b3717"
}
