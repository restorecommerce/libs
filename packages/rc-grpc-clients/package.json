{
  "name": "@restorecommerce/rc-grpc-clients",
  "version": "2.0.0",
  "description": "Restorecommerce GRPC service implementations",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "author": "n-fuse GmbH",
  "repository": {
    "type": "git",
    "url": "https://github.com/restorecommerce/libs.git"
  },
  "license": "MIT",
  "keywords": [
    "restore",
    "commerce",
    "grpc"
  ],
  "dependencies": {
    "@restorecommerce/grpc-client": "^1.0.2",
    "nice-grpc": "^2.0.0",
    "nice-grpc-common": "^2.0.0",
    "protobufjs": "^7.1.2",
    "rxjs": "^7.5.7",
    "ts-proto-descriptors": "^1.7.1"
  },
  "devDependencies": {
    "@restorecommerce/protos": "^2.0.0",
    "@types/jest": "^29.1.2",
    "@types/node": "^18.8.5",
    "jest": "^29.1.2",
    "npm-run-all": "^4.1.5",
    "protoc": "^1.0.4",
    "rimraf": "^3.0.2",
    "ts-jest": "^29.0.3",
    "ts-proto": "^1.128.0",
    "typescript": "^4.8.4"
  },
  "scripts": {
    "build": "npm-run-all build:clean build:compile",
    "build:clean": "rimraf -rf ./dist",
    "build:compile": "tsc -p tsconfig.lib.json",
    "prepublishOnly": "npm run build",
    "generate": "protoc --experimental_allow_proto3_optional --ts_proto_opt=outputClientImpl=false,env=node,useOptionals=true,outputJsonMethods=true,outputSchema=true,useExactTypes=false,outputServices=nice-grpc,outputServices=generic-definitions --proto_path=./node_modules/@restorecommerce/protos --plugin=./node_modules/.bin/protoc-gen-ts_proto --ts_proto_out=./src/generated ./node_modules/@restorecommerce/protos/io/restorecommerce/*.proto ./node_modules/@restorecommerce/protos/grpc/health/v1/*.proto ./node_modules/@restorecommerce/protos/grpc/reflection/v1alpha/*.proto ./node_modules/@restorecommerce/protos/test/*.proto ./node_modules/@restorecommerce/protos/echo/*.proto",
    "generate:google": "protoc --experimental_allow_proto3_optional --ts_proto_opt=outputClientImpl=false,env=node,useOptionals=true,outputJsonMethods=true,outputSchema=true,useExactTypes=false --proto_path=./node_modules/@restorecommerce/protos --plugin=./node_modules/.bin/protoc-gen-ts_proto --ts_proto_out=./src/generated ./node_modules/@restorecommerce/protos/google/protobuf/*.proto",
    "generate:server": "protoc --experimental_allow_proto3_optional --ts_proto_opt=stringEnums=true,outputClientImpl=false,env=node,useOptionals=true,outputJsonMethods=true,outputSchema=true,useExactTypes=false,outputServices=nice-grpc,outputServices=generic-definitions,snakeToCamel=false --proto_path=./node_modules/@restorecommerce/protos --plugin=./node_modules/.bin/protoc-gen-ts_proto --ts_proto_out=./src/generated-server ./node_modules/@restorecommerce/protos/io/restorecommerce/*.proto ./node_modules/@restorecommerce/protos/grpc/health/v1/*.proto ./node_modules/@restorecommerce/protos/grpc/reflection/v1alpha/*.proto ./node_modules/@restorecommerce/protos/test/*.proto ./node_modules/@restorecommerce/protos/echo/*.proto"
  },
  "engines": {
    "node": ">= 12.18.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "nx": {
    "targets": {
      "build": {
        "outputs": [
          "./dist"
        ]
      }
    }
  },
  "gitHead": "0a6f7c94bfb1811c5dd60ab8c4912a41299b3717"
}
