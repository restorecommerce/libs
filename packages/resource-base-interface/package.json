    "@restorecommerce/rc-grpc-clients": "^5.1.63",
    "@restorecommerce/scs-jobs": "^0.2.6",
    "@restorecommerce/service-config": "^1.1.7",
    "nice-grpc-server-reflection": "^3.0.2",
    "redis": "^4.7.0",
    "remeda": "^2.31.1"
  },
  "devDependencies": {
    "@restorecommerce/dev": "^0.2.0",
    "@types/nconf": "^0.10.7",
    "@vitest/coverage-v8": "^3.2.4",
    "arangojs": "^8.7.0",
    "cross-env": "^7.0.3",
