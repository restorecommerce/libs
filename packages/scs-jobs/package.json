{
  "name": "@restorecommerce/scs-jobs",
  "version": "0.0.1",
  "description": "Restorecommerce scs jobs",
  "main": "lib/index.js",
  "author": "n-fuse GmbH",
  "repository": {
    "type": "git",
    "url": "https://github.com/restorecommerce/libs.git"
  },
  "license": "MIT",
  "keywords": [
    "restore",
    "commerce",
    "grpc"
  ],
  "dependencies": {
    "bullmq": "^4.11.4",
    "lodash": "^4.17.21",
    "redis": "^4.6.8",
    "@restorecommerce/grpc-client": "^2.0.3",
    "@restorecommerce/kafka-client": "^1.1.1",
    "@restorecommerce/logger": "^1.2.4",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  },
  "scripts": {
    "start": "node lib/index.js",
    "dev": "cross-env NODE_ENV=development nodemon --watch './src/**/*.ts' --exec 'ts-node' src/index.ts",
    "dev:nocluster": "cross-env NODE_ENV=development nodemon --watch './src/**/*.ts' --exec 'ts-node' src/worker.ts",
    "test": "npm run lint && nyc npm run mocha",
    "lint": "eslint src --ext .ts",
    "mocha": "cross-env NODE_ENV=test mocha --full-trace --exit --timeout 30000",
    "test-debug": "npm run mocha -- --inspect-brk",
    "lcov-report": "nyc report --reporter=lcov",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "build:tsc": "tsc -d",
    "build:clean": "rimraf lib",
    "build": "npm-run-all lint build:clean build:tsc"
  },
  "engines": {
    "node": ">= 18.0.0"
  }
}
